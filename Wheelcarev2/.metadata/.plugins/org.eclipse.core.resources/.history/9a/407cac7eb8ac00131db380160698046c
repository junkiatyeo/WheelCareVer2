	function confirmGetMessage() {
	  var theAnswer = confirm("A notification will be sent!");

	  if (theAnswer){
	  	var welAnswer = confirm("Welcome aboard! Would you like to use the journey planner?");
	  	if (welAnswer){
	  		window.open("Select_Destination.html",'_parent');
	  	}
	  	else{
	  		window.open("Home.html",'_parent');
	  	}
	  }
	}
