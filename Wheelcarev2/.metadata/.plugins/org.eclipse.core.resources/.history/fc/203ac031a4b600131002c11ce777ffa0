<div id="vector" class="vector"  >
   	<input type="range" class="rangeC" name="points" id="points" value="0.8" step="0.2" min="0.4" max="0.8" width="200px" onchange="changeText()">
   	<script>
   	function changeText(){
   		var val = document.getElementById("points").value;
	   	
   		if (val == 0.4){
   			document.getElementById("u2").src="ListofNearbyBusStops4.html";
   		}
   		else if (val == 0.6){
   			document.getElementById("u2").src="ListofNearbyBusStops6.html";
   		}
   		else {
   			document.getElementById("u2").src="ListofNearbyBusStops.html";
   		}
   	}
   	</script>
   	
